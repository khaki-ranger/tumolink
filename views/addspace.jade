extends layout

block content
  form(method="post", action="/spaces")
    div
      h5 場所の名前
      input(type="text" name="spaceName")
    div
      h5 画像パス
      input(type="text" name="imgPath")
    div
      h5 Slack webhookURL
      input(type="text" name="slackWebhookURL")
    div
      h5 Slack channel
      input(type="text" name="slackChannel")
    button(type="submit") スペースをつくる
